files:
  "/var/log/httpd/lti_log":
    mode: "000600"
    owner: webapp
    group: webapp
    content: |
      [/var/log/httpd/lti_log]
      log_group_name = `{"Fn::Join":["/", ["/aws/elasticbeanstalk", { "Ref":"AWSEBEnvironmentName" }, "var/log/httpd/lti_log"]]}`
      log_stream_name = {instance_id}
      file = /var/log/httpd/lti_log*

